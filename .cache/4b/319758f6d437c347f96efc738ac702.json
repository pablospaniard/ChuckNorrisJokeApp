ontext(JokesContext)\n  const [name, setName] = useState('')\n  const [password, setPassword] = useState('')\n\n  const onLogInButtonClickHandler = () => {\n    dispatch({ type: actions.SET_AUTH, payload: true })\n    sessionStorage.setItem('user', JSON.stringify({ name, password }))\n  }\n\n  const notValid = errors(password).some(error => error.rule)\n\n  return (\n    <StyledDiv>\n      <FlexContainer>\n        <FlexItem>\n          <p>Username</p>\n          <Input type=\"text\" onChange={({ target }) => setName(target.value)} />\n        </FlexItem>\n        <FlexItem>\n          <p>Password</p>\n          <Input\n            type=\"text\"\n            maxLength=\"32\"\n            onChange={({ target }) => setPassword(target.value)}\n          />\n        </FlexItem>\n      </FlexContainer>\n      {errors(password).map(error => {\n        return (\n          <p\n            key={error.string}\n            css={css`\n              color: ${error.rule ? colors.red : colors.green};\n            `}\n          >\n            {error.string}\n          </p>\n        )\n      })}\n      <FlexContainer>\n        <FlexItem>\n          <Button\n            disabled={!name || !password || notValid}\n            onClick={onLogInButtonClickHandler}\n          >\n            Log In\n          </Button>\n        </FlexItem>\n      </FlexContainer>\n    </StyledDiv>\n  )\n}\n\nexport default Login\n"},"lineCount":null}},"error":null,"hash":"634199dce1e8989f28339fb9c942a4aa","cacheData":{"env":{}}}