\n          <input\n            type=\"text\"\n            onChange={({ target }) => setPassword(target.value)}\n          />\n        </FlexItem>\n      </FlexContainer>\n      <FlexContainer>\n        <FlexItem>\n          <Button\n            disabled={!name || !password}\n            onClick={onLogInButtonClickHandler}\n          >\n            Log In\n          </Button>\n        </FlexItem>\n      </FlexContainer>\n    </StyledDiv>\n  )\n}\n\nexport default Login\n"},"lineCount":null}},"error":null,"hash":"89c75fdd287849b917c3084d3c79b39b","cacheData":{"env":{}}}